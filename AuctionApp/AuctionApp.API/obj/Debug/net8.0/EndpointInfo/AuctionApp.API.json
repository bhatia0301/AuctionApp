    "/api/product/create-product": {
      "post": {
        "tags": [
          "Product"
        ],
        "requestBody": {
          "content": {
            "multipart/form-data": {
              "schema": {
                "required": [
                  "AuctionDuration",
                  "Category",
                  "Description",
                  "Name",
                  "ProductImage",
                  "ReservedPrice",
                  "StartingPrice"
                ],
                "type": "object",
                "properties": {
                  "Name": {
                    "maxLength": 100,
                    "minLength": 0,
                    "type": "string"
                  },
                  "Description": {
                    "maxLength": 500,
                    "minLength": 0,
                    "type": "string"
                  },
                  "StartingPrice": {
                    "minimum": 1,
                    "type": "number",
                    "format": "double"
                  },
                  "AuctionDuration": {
                    "maximum": 2147483647,
                    "minimum": 1,
                    "type": "integer",
                    "format": "int32"
                  },
                  "Category": {
                    "type": "string"
                  },
                  "ReservedPrice": {
                    "minimum": 1,
                    "type": "number",
                    "format": "double"
                  },
                  "ProductImage": {
                    "type": "string",
                    "format": "binary"
                  }
                }
              },
              "encoding": {
                "Name": {
                  "style": "form"
                },
                "Description": {
                  "style": "form"
                },
                "StartingPrice": {
                  "style": "form"
                },
                "AuctionDuration": {
                  "style": "form"
                },
                "Category": {
                  "style": "form"
                },
                "ReservedPrice": {
                  "style": "form"
                },
                "ProductImage": {
                  "style": "form"
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "text/plain": {
                "schema": {
                  "$ref": "#/components/schemas/ResponseDTO"
                }
              },
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ResponseDTO"
                }
              },
              "text/json": {
                "schema": {
                  "$ref": "#/components/schemas/ResponseDTO"
                }
              }
            }
          }
        }
      }
    },
